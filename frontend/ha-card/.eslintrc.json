{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["warn", {"argsIgnorePattern": "^_"}],
    "no-console": "off",
    "semi": ["error", "always"],
    "quotes": ["error", "double", {"avoidEscape": true, "allowTemplateLiterals": true}]
  }
}
